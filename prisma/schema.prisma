generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}



model User {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  email String @unique
  hashedPassword String?
  name String 
  createdAt DateTime @default(now())
  updateAt DateTime @updatedAt
  

}